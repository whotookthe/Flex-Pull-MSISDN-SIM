javascript:(function() {let extractedData = []; function findNextTextContent(element) {let nextElement = element.nextElementSibling; return nextElement ? nextElement.textContent.trim() : null;} let elements = document.querySelectorAll('p, div, span'); console.log('Scanning page for Line Numbers and SIM Serial Numbers...'); elements.forEach(function(element) {let text = element.textContent.trim(); console.log(`Checking element: "${text}"`); if (text.includes('Line Number')) {let lineNumber = findNextTextContent(element); if (lineNumber) {extractedData.push(lineNumber); console.log(`Found Line Number: ${lineNumber}`);}} if (text.includes('SIM Serial Number')) {let simSerial = findNextTextContent(element); if (simSerial) {extractedData.push(simSerial); console.log(`Found SIM Serial Number: ${simSerial}`);}}}); if (extractedData.length === 0) {console.warn('No matching Line Number or SIM Serial Number found.');} else {let fileContent = extractedData.join('\n'); console.log('Extracted Data:', fileContent); let blob = new Blob([fileContent], { type: 'text/plain' }); let link = document.createElement('a'); link.href = window.URL.createObjectURL(blob); link.download = 'extracted_data.txt'; document.body.appendChild(link); link.click(); document.body.removeChild(link);}})();
